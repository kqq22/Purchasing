  <select id="selectQuoteAll" resultMap="BaseResultMap">
    SELECT * from quote q
    LEFT JOIN id_mapping i ON q.id=i.QUOTE_ID
    LEFT JOIN stock s on i.STOCK_ID=s.id
    LEFT JOIN enquire e ON e.ID=i.ENQUIRE_ID
    limit ${curPage},${pageSize}
  </select>

  <select id="getCount" resultMap="ccount">
   SELECT count(*) count from quote q
    LEFT JOIN id_mapping i ON q.id=i.QUOTE_ID
    LEFT JOIN stock s on i.STOCK_ID=s.id
    LEFT JOIN enquire e ON e.ID=i.ENQUIRE_ID
  </select>

  <select id="selectQuoteById" resultMap="BaseResultMap">
    SELECT * from quote q
    LEFT JOIN quote_detail d ON q.ID=d.QUOTE_ID
    WHERE q.ID=${id}
  </select>

  <select id="selectQuoteByStatus" resultMap="BaseResultMap">
    SELECT * from quote q
    LEFT JOIN id_mapping i ON q.id=i.QUOTE_ID
    WHERE i.STATUS='C001-100'
    limit ${curPage},${pageSize}
  </select>

  <select id="getCountByStatus" resultMap="ccount">
    SELECT count(*) count from quote q
    LEFT JOIN id_mapping i ON q.id=i.QUOTE_ID
    WHERE i.STATUS='C001-100'
  </select>

  <select id="selectQuoteByIdToContract" resultMap="BaseResultMap">
    SELECT * from quote q
    LEFT JOIN id_mapping i ON q.id=i.QUOTE_ID
    LEFT JOIN orders o on i.ORDER_ID=o.id
    LEFT JOIN quote_detail d ON d.QUOTE_ID=q.ID
    WHERE q.ID=${id}
  </select>

